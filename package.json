    "pjson": "^1.0.9",
    "request": "^2.85.0",
    "rimraf": "^2.6.2",
    "which": "^1.3.0",
    "winston": "^2.4.1",
    "yamljs": "^0.3.0",
    "zen-observable": "^0.8.8"
