{
  "name": "double-cli",
  "version": "0.1.0",
  "description": "Double CLI - Terminal access for Double, the simplest blockchain development environment",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "lint": "tslint",
    "test": "rimraf dist && tsc && tslint -c tslint.json src/**/*.ts && nyc ava",
    "clean": "rimraf dist",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "bin": {
    "double": "dist/cli.js"
  },
  "dependencies": {
    "ajv": "^6.4.0",
    "chalk": "^2.3.2",
    "commander": "^2.15.1",
    "listr": "^0.14.0",
    "lodash": "^4.17.5",
    "mkdirp": "^0.5.1",
    "pjson": "^1.0.9",
    "rimraf": "^2.6.2",
    "which": "^1.3.0",
    "yamljs": "^0.3.0",
    "zen-observable": "^0.8.8"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/listr": "^0.13.0",
    "@types/lodash": "^4.14.106",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^9.6.2",
    "@types/rimraf": "^2.0.2",
    "@types/which": "^1.3.1",
    "@types/yamljs": "^0.2.30",
    "ava": "^1.0.0-beta.3",
    "coveralls": "^3.0.0",
    "install": "^0.11.0",
    "npm": "^5.8.0",
    "nyc": "^11.6.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doubleproject/double-cli.git"
  },
  "bugs": {
    "url": "https://github.com/doubleproject/double-cli/issues"
  },
  "keywords": [
    "cli",
    "cli-app",
    "blockchain",
    "crypto",
    "ethereum"
  ],
  "license": "MIT"
}
